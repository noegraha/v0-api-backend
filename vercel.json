{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/simpan-data",
      "dest": "/api/simpan-data.js"
    },
    {
      "src": "/api/get-laporan",
      "dest": "/api/get-laporan.js"
    },
    {
      "src": "/api/random-quote",
      "dest": "/api/random-quote.js"
    },
    {
      "src": "/api/db-test",
      "dest": "/api/db-test.js"
    },
    {
      "src": "/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/ping",
      "dest": "/api/ping.js"
    },
    {
      "src": "/api",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}